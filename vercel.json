{
  "buildCommand": "npm run tsc",
  "devCommand": "npm run dev",
  "framework": null,
  "installCommand": "npm install",
  "outputDirectory": "api",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/api/docs"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)*",
      "destination": "/api"
    }
  ],
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@now/node"
    }
  ]
}
